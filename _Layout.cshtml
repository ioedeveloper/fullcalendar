                    '<span class="message-content">' + message +
                    '</span>' +
                    '</div>' +
                    '</div>';
                return html;
            }
        }

        //create format for message to display in chat box
        function chatformat_msg(msg, name, time) {

            if (name === username) {

                if (msg === '') {
                    return "";
                }
                else {
                    html = '<div class="right">';
                    html += '<div class="author-name">' + 'You' + ' ';
                    html += '<small class="chat-date">' + time;
                    html += '</small>';
                    html += '</div>';
                    html += '<div class="chat-message">' + msg;
                    html += '</div>';
                    html += '</div>';
                    return html;
                }
            } else {
                if (msg === '') {
                    return "";
                }
                else {
                    html = '<div class="left">';
                    html += '<div class="author-name">' + name + ' ';
                    html += '<small class="chat-date">' + time;
                    html += '</small>';
                    html += '</div>';
                    html += '<div class="chat-message active">' + msg;
                    html += '</div>';
                    html += '</div>';
                    return html;
                }
            }
        }

