        //formatGroupChat function to format group chat
        function formatGroupChat(name, message) {
            var d = new Date();
            var h = d.getHours();
            var m = d.getMinutes();
            if (name === username) {
                var html = '<div class="chat-message right">' +
                    '<img class="message-avatar" src="/images/DP/dummy.png" alt="">' +
                    '<div class="message">' +
                    '<a class="message-author" href="#">' + name + '</a>' +
                    '<span class="message-date">' + h + ':' + m + '</span>' +
                    '<span class="message-content">' + message +
                    '</span>' +
                    '</div>' +
                    '</div>';
                return html;
            } else {
                var html = '<div class="chat-message left">' +
                    '<img class="message-avatar" src="/images/DP/dummy.png" alt="">' +
                    '<div class="message">' +
                    '<a class="message-author" href="#">' + name + '</a>' +
