                    }
                }

            };

            //Function initiated by the server side due to sent group message by another user
            chat.client.addNewMessageToPage = function (name, message, userCount, list) {
                var d = new Date();
                var h = d.getHours();
                var m = d.getMinutes();

                var html = formatGroupChat(name, message);
                $('.chat-discussion').append(html);
                $(".chat-discussion").scrollTop($(".chat-discussion")[0].scrollHeight);
            };

            //Call back function for hub.start making a call to ChatHub class (c#)
            $.connection.hub.start().done(function () {
                var userId = '@User.Identity.GetUserId()';
                var conId = $.connection.hub.id;
                chat.server.registerUser(userId, conId);
                //calls getUser on ChatHub class (c#) which initiates chat.client.onGetUser
                chat.server.getUser();
            });

            //Enter keypress function to listen and send private message
            $(document).on('keypress', ".chatMessage", function (e) {
                if (e.keyCode == 13) {
                    var id = $(this).attr("data-user-id");
                    var msg = $(this).val();
                    var userId = '@User.Identity.GetUserId()';
                    //send message to recipient
                    chat.server.sendPrivateMessage(id, msg, userId);
                    var d = new Date();
                    var h = d.getHours();
                    var m = d.getMinutes();
                    var time = h + ':' + m;
                    msg = chatformat_msg(msg, username, time);
                    //Add message to senders chat window
                    $(".message-area #user-window" + id + " .content").append(msg);
                    $(this).val("");
                    $(this).focus();
                    $(".message-area #user-window" + id + " .content").scrollTop($(".message-area #user-window" + id + " .content")[0].scrollHeight);
                    e.preventDefault();
                }
            });

            //Enter keypress function to listen and send group message
            $(document).on('keypress', ".message-input", function (e) {
                if (e.keyCode === 13) {
                    var msg = $(this).val();
                    //send message to recipient
                    chat.server.send(username, msg);
                    $(this).val('').focus();
                    e.preventDefault();

                }
            });

            $('#small-chat').on('click', function () {
                $('.message-area').toggleClass('showit');
            });
        });

